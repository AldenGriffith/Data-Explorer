Panel.Area <- 1.41    #individual solar panel size (m2)
Num.Panels <- 48  #there are 48 panels
Tilt <- 25
Aspect <- 155
#Total panel area
Total.Area = Panel.Area*Num.Panels
#Total loop length (total time)
